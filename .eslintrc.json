{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "prettier",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        // "prettier/prettier": ["warn", {"singleQuote": true,  "endOfLine":"auto"}],
        "camelcase": "off",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "capitalized-comments": "off",
        "no-unused-vars": "warn",
        "react/prop-types": "off"
    }
}
